<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jvwl.model.Log" table="t_log">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="username" length="30" not-null="true"/>
		<property name="time" length="20" not-null="true"/>
		<property name="content"/>
	</class>
</hibernate-mapping>